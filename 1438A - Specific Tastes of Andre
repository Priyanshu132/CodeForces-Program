// Code: 


import java.util.*;
public class Solution
{
	public static void main(String[] args) {
	    
	    Scanner s = new Scanner(System.in);
	    
	    int t = s.nextInt();
	    
	    while(t-- > 0){
	     
	        int n = s.nextInt();
	        
	        while(n-- > 0){
	            	System.out.print("1 ");
	        }
	        	System.out.println();
	    }
	 
	    
	
	}
}
